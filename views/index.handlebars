  <!-- HandleBars -->
  <div class="row">
    <div class="col-md-1">
    </div>
    <div class="col-md-5">
      <p>on order:</p>
      <ul class="list-group">
        {{#each taco}}
        {{#unless devoured}}
        <li class="list-group-item text-left">
          <span class="align-middle">{{this.id}}<span>. </span>{{this.taco_name}}</span> <span><button data-id="{{this.id}}" class="devour btn btn-secondary mb-2 float-right">Devour</button></span>
        </li>
        {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="col-md-5">
      <p>eaten:</p>
      <ul class="list-group">
        {{#each taco}}
        {{#if devoured}}
        <li class="list-group-item text-left disabled">
          <span class="align-middle">{{this.id}}<span>. </span>{{this.taco_name}}</span> <span><button data-id="{{this.id}}" class="delete btn btn-secondary mb-2 float-right">Delete</button></span>
        </li>
        {{/if}}
        {{/each}}
      </ul>
    </div>

    <div class="col-md-1">
    </div>
  </div>
